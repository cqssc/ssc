    <SolidColorBrush x:Key="SystemBrush1" Color="{DynamicResource SystemColor1}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush2" Color="{DynamicResource SystemColor2}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush3" Color="{DynamicResource SystemColor3}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush4" Color="{DynamicResource SystemColor4}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush5" Color="{DynamicResource SystemColor5}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush6" Color="{DynamicResource SystemColor6}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush7" Color="{DynamicResource SystemColor7}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush8" Color="{DynamicResource SystemColor8}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush9" Color="{DynamicResource SystemColor9}"></SolidColorBrush>
    <SolidColorBrush x:Key="SystemBrush10" Color="{DynamicResource SystemColor10}"></SolidColorBrush>
    <ImageBrush x:Key="SystemBrush11" ImageSource="bg.jpg" Stretch="Fill"></ImageBrush>

    <Style x:Key="FocusVisual">
        <Setter Property="Control.Template">
            <Setter.Value>
                <ControlTemplate>
                    <Border>
                        <Rectangle Margin="15,0,0,0"
                       StrokeThickness="1"
                       Stroke="{StaticResource SystemBrush7}"
                       StrokeDashArray="1 2" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <LinearGradientBrush x:Key="ProgressBarIndicatorAnimateBrush"
                         StartPoint="0,0" EndPoint="1,0">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource SystemColor5}"
                      Offset="0" />
                <GradientStop Color="{DynamicResource SystemColor7}"
                      Offset="0.4" />
                <GradientStop Color="{DynamicResource SystemColor8}"
                      Offset="0.6" />
                <GradientStop
                    Offset="1" Color="{DynamicResource SystemColor10}">
                </GradientStop>
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <Style TargetType="{x:Type Control}">
        <Setter Property="FontFamily" Value="Microsoft Yahei"></Setter>
        <Setter Property="FontSize" Value="12"></Setter>
    </Style>
    
    <Style x:Key="ExpandCollaspToggleStyle" TargetType="{x:Type ToggleButton}">
        <Setter Property="Focusable" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Grid Width="15"
                            Height="13"
                            Background="Transparent">
                        <Path x:Name="Collapsed"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="1,1,1,1"
                                  Data="M 4 0 L 8 4 L 4 8 Z"
                              Fill="{DynamicResource SystemBrush3}">
                        </Path>
                        <Path x:Name="Expanded"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Center"
                                  Margin="1,1,1,1"
                                  Data="M 0 4 L 8 4 L 4 8 Z"
                                  Visibility="Hidden"
                              Fill="{DynamicResource SystemBrush3}">
                        </Path>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="Collapsed" Property="Visibility" Value="Collapsed"></Setter>
                            <Setter TargetName="Expanded" Property="Visibility" Value="Visible"></Setter>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Collapsed" Property="Fill" 
                                    Value="{DynamicResource SystemBrush7}"></Setter>
                            <Setter TargetName="Expanded" Property="Fill"
                                    Value="{DynamicResource SystemBrush7}"></Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!--FormStyle-->
    
    <Style x:Key="FormBorderItem" TargetType="{x:Type Border}">
        <Setter Property="Margin" Value="2,5,10,5"></Setter>
        <Setter Property="HorizontalAlignment" Value="Left"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="MinWidth" Value="80"></Setter>
    </Style>
    
    <Style x:Key="FormBorderValue" TargetType="{x:Type Border}">
        <Setter Property="Margin" Value="2,5"></Setter>
        <Setter Property="HorizontalAlignment" Value="Left"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
    </Style>

    <Style x:Key="FormButtonSeperator" TargetType="{x:Type Border}">
        <Setter Property="Height" Value="2"></Setter>
        <Setter Property="Background" Value="{DynamicResource SystemBrush7}"></Setter>
        <Setter Property="Margin" Value="10 5"></Setter>
    </Style>

    <Style x:Key="FormButtonStyle" TargetType="{x:Type Border}">
        <Setter Property="HorizontalAlignment" Value="Center"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="Width" Value="80"></Setter>
        <Setter Property="Height" Value="25"></Setter>
        <Setter Property="Margin" Value="10,5,10,15"></Setter>
    </Style>

    <!--FormStyle-->

</ResourceDictionary>